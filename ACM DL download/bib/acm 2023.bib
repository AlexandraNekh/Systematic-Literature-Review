pages = {5859–5860},
numpages = {2},
keywords = {machine learning management, ml product development, data science management},
location = {<conf-loc>, <city>Long Beach</city>, <state>CA</state>, <country>USA</country>, </conf-loc>},
series = {KDD '23}
}

