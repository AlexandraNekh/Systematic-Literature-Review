@inproceedings{10.1145/3585088.3593884,
author = {Adisa, Ibrahim Oluwajoba and Thompson, Ian and Famaye, Tolulope and Sistla, Deepika and Bailey, Cinamon Sunrise and Mulholland, Katherine and Fecher, Alison and Lancaster, Caitlin Marie and Arastoopour Irgens, Golnaz},
title = {S.P.O.T: A Game-Based Application for Fostering Critical Machine Learning Literacy Among Children},
