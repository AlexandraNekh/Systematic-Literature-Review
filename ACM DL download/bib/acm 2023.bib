@inproceedings{10.1145/3568294.3579951,
author = {Mohan, Mayumi and Neerincx, Anouk and Alves-Oliveira, Patricia and Fitter, Naomi T.},
title = {Workshop YOUR Study Design 2023! Participatory Critique and Refinement of Participants' Studies},
year = {2023},
isbn = {9781450399708},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3568294.3579951},
