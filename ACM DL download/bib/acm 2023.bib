@article{10.1145/3610168,
author = {Nisi, Valentina and Bala, Paulo and Ces\'{a}rio, Vanessa and James, Stuart and Del Bue, Alessio and Nunes, Nuno Jardim},
title = {"Connected to the People": Social Inclusion &amp; Cohesion in Action through a Cultural Heritage Digital Tool},
year = {2023},
issue_date = {October 2023},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {7},
number = {CSCW2},
url = {https://doi.org/10.1145/3610168},
doi = {10.1145/3610168},
abstract = {Current cultural policies are evolving from social inclusion (removing barriers and promoting equality for participation in culture) to social cohesion (fostering solid bonds between groups despite their differences). Digital interventions can create spaces that promote social inclusion and cohesion. In this paper, we report on the design and evaluation of a cultural heritage and digital storytelling application supporting a participatory approach to culture and hosting society. We evaluate our intervention in three marginalized communities with different social-cultural contexts: migrant women in Barcelona, a community living in a priority neighbourhood in Paris and second and third-generation migrants in Lisbon. Through an analysis of their application use, our findings point at their needs and desires, highlighting how the app can support social inclusion as the first step towards cohesion, but that these are heterogeneous concepts susceptible to nuanced appropriations by the different communities.},
journal = {Proc. ACM Hum.-Comput. Interact.},
month = {oct},
articleno = {319},
numpages = {37},
keywords = {migrants, cultural exclusion, cultural heritage, digital storytelling, communities, social inclusion, social cohesion}
}

