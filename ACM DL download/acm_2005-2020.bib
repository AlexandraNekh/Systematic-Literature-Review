title = {Interactive Interface and Serious Game for the Detection of Deficiencies in Cognitive Functions in Preschool Children},
year = {2018},
isbn = {9781450366533},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3293578.3298777},
doi = {10.1145/3293578.3298777},
abstract = {The timely identification of deficiencies in cognitive functions, such as attention problems, are of vital importance for children to acquire skills such as literacy. However, today, the application of psychometric tests for the identification of attention problems takes place when children already have symptoms, or problems associated with the development of their learning. So, an early detection in the preschool age is of vital importance for their optimal development. The use of serious games and interactive interfaces for the detection and therapy of problems related to learning has helped to obtain better results according to the studies analyzed in the related work. In this work we propose proposed to identify deficiencies in cognitive functions for preschool children, using a serious game and a control interface that provides a natural interaction. Following the first stages of the experimental research methodology, the methodology of soft systems, the incremental model of software development and the rapid contextual design technique, a prototype of interactive interface was obtained and the proposal of a serious game with the necessary characteristics for performing tests to detect deficiencies in cognitive functions.},
booktitle = {Proceedings of the 7th Mexican Conference on Human-Computer Interaction},
articleno = {22},
numpages = {4},
keywords = {cognitive deficiencies, human computer interaction, Attention, evaluation of cognitive functions},
location = {Merida, Mexico},
series = {MexIHC '18}
}

@inproceedings{10.1145/3064663.3064750,
author = {Fuchsberger, Verena and Meneweger, Thomas and Wurhofer, Daniela and Tscheligi, Manfred},
title = {Apply Now! Fictional Job Postings as an Instrument to Discuss Interactive Futures of Work},
year = {2017},
isbn = {9781450349222},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3064663.3064750},
doi = {10.1145/3064663.3064750},
abstract = {Design fiction provides us with a variety of ways to explore potential interactive futures. In order to reflect on the future of workplaces, we created fictional job postings for the context of production facilities. This context is characterized by many constraints, which are often hard to overcome when envisioning the future of interactive practices in such environments. With the fictional job postings, we triggered discussions with stakeholders from a production site and with the public -- which both resulted in highly controversial debates. In this note, we present the evolution of an exemplary job posting and the reactions it evoked. We then discuss our learnings (e.g., that the degree of fiction is essential) and conclude by reflecting on how this instrument facilitates the exploration of prospective interactive practices at work.},
booktitle = {Proceedings of the 2017 Conference on Designing Interactive Systems},
pages = {581–586},
numpages = {6},
keywords = {work places, design fiction, computer-supported practices},
location = {Edinburgh, United Kingdom},
series = {DIS '17}
}

@inproceedings{10.1145/1514402.1514403,
author = {Bilda, Zafer and Bowman, Chris and Edmonds, Ernest},
title = {Experience Evaluation of Interactive Art: Study of GEO Landscapes},
year = {2008},
isbn = {9781605584249},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/1514402.1514403},
doi = {10.1145/1514402.1514403},
abstract = {The cutting edge in the digital arts is a highly fertile ground for the investigation of engagement and the role of new technologies. The nature of audience interaction with artworks and the implication for the consequential nature of the interactive art system is itself important. This paper examines the process of experience evaluation of interactive art and its role in improving design of interactive art systems. The experience evaluation was based upon the case-study of an interactive artwork entitled GEO Landscapes .01 -- a gallery installation prototype which commenced development in 2006 and was exhibited in a public space in 2007. The experience evaluation study demonstrated how the novice versus expert profiles of interaction behaviours influenced the nature of the artwork experience. The research findings improved our understanding of how people experience artworks and also had an impact on the experience design of GEO Landscapes and the future direction of the project.},
booktitle = {Proceedings of the 5th Australasian Conference on Interactive Entertainment},
articleno = {1},
numpages = {10},
keywords = {evaluation, audience experience, interaction design, interactive art},
location = {Brisbane, Queensland, Australia},
series = {IE '08}
}

