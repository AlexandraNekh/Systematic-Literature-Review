keywords = {Gesture Recognition, Augmentative and Alternative Communication, Accessibility, Assistive Technology, Computer Vision},
location = {Bel\'{e}m, Brazil},
series = {IHC 2018}
}

@inproceedings{10.1145/3429290.3429306,
author = {Ashwini, B},
title = {Robot Assisted Diagnosis for Autism in Children},
year = {2021},
isbn = {9781450389440},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3429290.3429306},
doi = {10.1145/3429290.3429306},
abstract = {The complexity of the diagnostic procedure coupled with the absence of medical markers poses a variety of challenges in the early detection of autism in children. The diagnosis relies on the observation of behavioural symptoms and parent reporting of the developmental history of the child. It requires years of experience and training for identifying these symptoms which could be often misjudged as other mental conditions with similar symptomology. In this work, we propose a robot-assisted diagnostic protocol for the early detection of autism leveraging the onboard sensors of Socially Assistive Robots (SAR) and the vast potential of machine learning techniques. The robot assistant's role will be two-fold: (a) supporting the expert in administering the diagnostic procedure and (b) evaluating the behavioural symptoms and reaching a reliable diagnosis. Even though similar studies have been done in the Global North, the challenges posed by the social and cultural diversity of the Indian population is varied and needs to be explored further.},
booktitle = {Proceedings of the 11th Indian Conference on Human-Computer Interaction},
pages = {82–85},
numpages = {4},
keywords = {Socially Assistive Robots, Autism Spectrum Disorder, Robot Assisted Intervention, Multi- modal Interaction},
location = {Online, India},
series = {IndiaHCI '20}
}

@inproceedings{10.5555/1109180.1109211,
author = {Szilas, Nicolas},
title = {The Future of Interactive Drama},
year = {2005},
isbn = {0975153323},
publisher = {Creativity &amp; Cognition Studios Press},
address = {Sydney, AUS},
abstract = {Interactive Drama is the ultimate challenge of digital entertainment. In this paper, from our seven year experience in Interactive Drama, we try to shape the history of the field and envision what will be (or should be) the future of this history. Two main directions in particular are stressed, because we feel that the success of Interactive Drama lies in these two directions. The first one concerns the architecture of systems and how it would manage both narrative constraints and character's intelligence, believability and roundness. The second one focuses on project management by sketching a methodology of co-design for Interactive Drama.},
