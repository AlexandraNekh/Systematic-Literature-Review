pages = {353–354},
numpages = {2},
keywords = {creative environment, information access and management, problem finding, creativity, design, collaboration},
location = {Berkeley, California, USA},
series = {C&amp;C '09}
}

@inproceedings{10.1145/3397617.3398059,
