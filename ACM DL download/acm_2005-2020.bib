@inproceedings{10.1145/2038476.2038484,
author = {Protopsaltis, Aristidis and Auneau, Laurent and Dunwell, Ian and de Freitas, Sara and Petridis, Panagiotis and Arnab, Sylvester and Scarle, Simon and Hendrix, Maurice},
title = {Scenario-Based Serious Games Repurposing},
year = {2011},
