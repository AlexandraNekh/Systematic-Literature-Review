@inproceedings{10.5555/1690508.1690546,
author = {Pommeranz, Alina and Brinkman, Willem-Paul and Wiggers, Pascal and Broekens, Joost and Jonker, Catholijn M.},
title = {Design Guidelines for Negotiation Support Systems: An Expert Perspective Using Scenarios},
year = {2009},
isbn = {9789513863401},
publisher = {VTT Technical Research Centre of Finland},
address = {FI-02044 VTT, FIN},
abstract = {Negotiation support systems (NSS) can enhance humans' performance in negotiations. Much research in this area focuses on finding optimal bids. However, there is little research on human factors in technological negotiation support. We believe an in-depth analysis of the task involving experts and users is needed to build a new generation of NSS focusing on man-machine collaboration. We describe a scenario-based approach to gathering requirements for such a system. We wrote five scenarios containing part of the envisioned functionality in the most important use situations, e.g. face-to-face negotiation, on the phone, collaborative or mobile preparation. We used claims analysis to clarify our design decisions. To evaluate our claims we organized focus groups including six general and six job negotiation experts. The filmed scenarios were used together with two claims each to guide the discussion. Based on the data analysis we constructed 12 design guidelines for NSS.},
booktitle = {European Conference on Cognitive Ergonomics: Designing beyond the Product --- Understanding Activity and User Experience in Ubiquitous Environments},
articleno = {27},
numpages = {8},
keywords = {decision making and problem solving, decision aiding, design approaches, human-system interaction},
location = {Helsinki, Finland},
series = {ECCE '09}
}

@inproceedings{10.1145/3334480.3375161,
