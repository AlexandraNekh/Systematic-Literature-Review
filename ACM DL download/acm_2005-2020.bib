pages = {59–66},
numpages = {8},
keywords = {learning objects, participatory design, multimedia authoring},
location = {Jo\~{a}o Pessoa, Brazil},
series = {WebMedia '14}
}

@inproceedings{10.1145/1980822.1980826,
author = {Anticoli, Linda and Toppano, Elio},
