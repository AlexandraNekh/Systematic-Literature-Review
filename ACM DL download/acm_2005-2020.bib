url = {https://doi.org/10.1145/2723839.2723858},
doi = {10.1145/2723839.2723858},
abstract = {This paper presents a framework for context-aware processes on factory shop floors, in which the context used for adapting processes is created directly by the workers involved in the process. It departs from other approaches to building context-aware systems where the context is pre-defined by modelling experts not directly involved in the process. The paper focuses on techniques and tools for capturing people-centered context and using it for adapting processes to the needs of human workers. This framework forms the basis for a set of mockups, prototypes and conceptual models developed within SO-PC-Pro, an ongoing EU FP7 "Factories of the Future" research project.},
booktitle = {Proceedings of the 7th International Conference on Subject-Oriented Business Process Management},
articleno = {15},
numpages = {5},
keywords = {context-awareness, sensors, S-BPM, context},
location = {Kiel, Germany},
series = {S-BPM ONE '15}
}

@inproceedings{10.5555/2384470.2384494,
author = {Ramic-Brkic, Belma and Karkin, Zana and Sadzak, Aida and Selimovic, Dino and Rizvic, Selma},
title = {Augmented Real-Time Virtual Environment of the Church of the Holy Trinity in Mostar},
year = {2009},
isbn = {9783905674187},
publisher = {Eurographics Association},
address = {Goslar, DEU},
abstract = {Digital storytelling significantly improves the immersion of the users into virtual environments. The perception of the information contained in the digital story is better perceived if the story is told by a real avatar, rather than the animated character. The paper describes how we improved the approach of inserting the real avatar recorded against a green screen using a sequence of images with an alpha channel in an X3D real time virtual environment.},
