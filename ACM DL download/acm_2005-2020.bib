@inproceedings{10.1145/3313831.3376258,
author = {Lin, Yuyu and Guo, Jiahao and Chen, Yang and Yao, Cheng and Ying, Fangtian},
title = {It Is Your Turn: Collaborative Ideation With a Co-Creative Robot through Sketch},
year = {2020},
isbn = {9781450367080},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3313831.3376258},
doi = {10.1145/3313831.3376258},
abstract = {Co-creative systems have been widely explored in the field of computational creativity. However, existing AI partners of these systems are mostly virtual agents. As sketching on paper with embodied robots could be more engaging for designers' early-stage ideation and collaborative practices, we envision the possibility of Cobbie, a mobile robot that ideates iteratively with designers by generating creative and diverse sketches. To evaluate the differences in co-creativity and user experience between the co-creative robots and virtual agents, we conducted a comparative experiment and analyzed the data collected from quantitative scales, observation, and semi-structured interview. The results reveal that Cobbie is more satisfying in motivating exploration, provoking unexpected ideas and engaging designers in the collaborative ideation process. Based on these findings, we discussed the prospects of co-creative robots for future developments of human-AI collaborative systems.},
booktitle = {Proceedings of the 2020 CHI Conference on Human Factors in Computing Systems},
pages = {1–14},
numpages = {14},
keywords = {creative robot, co-creative system, ideation, human-ai collaboration, early-stage design},
location = {Honolulu, HI, USA},
series = {CHI '20}
}

@inproceedings{10.1145/2987491.2987516,
author = {Bolton, Anthony and Goosen, Leila and Kritzinger, Elmarie},
title = {Enterprise Digitization Enablement Through Unified Communication &amp; Collaboration},
year = {2016},
isbn = {9781450348058},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/2987491.2987516},
doi = {10.1145/2987491.2987516},
abstract = {This paper discusses the emerging paradigm of the Internet of Things (IoT), focusing on the important role that people will play in the future connected world of digitized services and electronic "Things". The associated research highlights how IoT is influencing changes and requirements in communication required to facilitate decision making between systems, devices and people in a connected world. The paper provides background context on the development of IoT technologies and how these developments overlap with industry, enabling revolutionary changes such as Industry 4.0 and the advent of the Industrial Internet. The emergence of IoT technologies, IoT enabled industry and IoT influenced social change are discussed, highlighting the new requirements and production demands that are emerging for integrated communications, features and systems.The paper introduces a new end-to-end and framework for enterprise planning, adoption and deployment of a feature rich and integrated Unified Communications and Collaboration (UC&amp;C) system within a multinational enterprise. The new framework presents a multi-domain structure with four primary modular containers. This structured model introduces new concepts of a Core Governance Domain (CGD), Operational Management Domain (OMD), Unified Services Domain (USD) and Integrated Feature Domain (IFD). Combined these domains represent a coherent framework that supports initial deployment and planning, ongoing sustain and technical agile technical architecture. This research has been carried out within the scope of a multi-year automotive focused study in General Motors.},
booktitle = {Proceedings of the Annual Conference of the South African Institute of Computer Scientists and Information Technologists},
articleno = {6},
numpages = {10},
keywords = {Collaboration, Internet, SIP, IoT, Framework, Modular, Integration, Communication, Deployment, Unified, Session, People, Digitization, Things, Architecture},
location = {Johannesburg, South Africa},
series = {SAICSIT '16}
}

