  booktitle={2023 International Conference on Distributed Computing and Electrical Circuits and Electronics (ICDCECE)}, 
  title={A Comprehensive Overview on Musical Therapy Using Facial Expression Recognition}, 
  year={2023},
  volume={},
  number={},
  pages={1-5},
  abstract={In recent years, music therapy has faced the challenge of research. Not only is there a tradition of quantitative research, but qualitative research approaches have also been incorporated into the field, as is necessary for a clinical approach that includes science and art, which greatly impacts the workings of the human brain. A new approach to auto-play music using facial emotions. Most existing approaches involve manually playing music, using portable computing devices, or classification based on audio features. Instead, we suggest changing to manual sorting and playback. This approach uses CNN for emotion detection and CK+ is used for facial expression detection classification.},
  keywords={},
  doi={10.1109/ICDCECE57866.2023.10151044},
  ISSN={},
  month={April},}@INPROCEEDINGS{9889396,
  author={Mohan, Mayumi and Neerincx, Anouk and Zaga, Cristina and Fitter, Naomi T.},
  booktitle={2022 17th ACM/IEEE International Conference on Human-Robot Interaction (HRI)}, 
  title={Workshop YOUR study design! Participatory critique and refinement of participants' studies}, 
  year={2022},
  volume={},
  number={},
  pages={1281-1283},
  abstract={HRI is an interdisciplinary field that requires researchers to be knowledgeable in broad areas ranging from social sciences to engineering. Study design is a multifaceted aspect of HRI that is hard to develop and perfect. Thus, the second edition of the “Workshop Your Study Design” workshop aims to improve the quality of future HRI studies by training researchers and boosting the accessibility of HRI as a field. Participants will have the opportunity to receive guidance and feedback on their study from an expert mentor. Researchers from all avenues of HRI will be invited to submit a 2–4 page paper on an HRI study they are currently designing, including a brief introduction and a complete methods section. Accepted submissions will be discussed in small groups led by mentors with relevant expertise. Prior to the workshop, papers will be shared within each group. Participants will be encouraged to read other submissions. During the workshop, attendees will work within their menteementor groups to discuss each paper and provide feedback. There will also be a session where mentors lead mini discussions on topics important to study design, such as balancing qualitative and quantitative design, power analysis, and research ethics. The workshop will end with a session where all participants can share important lessons that they learned with fellow attendees.},
  keywords={},
  doi={10.1109/HRI53351.2022.9889396},
  ISSN={},
  month={March},}@INPROCEEDINGS{9742510,
  author={Zheng, Penghua},
  booktitle={2021 2nd International Conference on Information Science and Education (ICISE-IE)}, 
  title={Research on the cultivation of professional system ability in Computer Education}, 
  year={2021},
  volume={},
  number={},
  pages={1281-1284},
  abstract={Computer majors have high requirements for students' practical ability, but now many colleges and universities prefer theoretical teaching when training computer majors, and the curriculum arrangement is not clear, resulting in students' lack of computer professional system ability. At present, the embedded system, mobile computing system, parallel computing system and other new computing systems in the computer system are developing rapidly. When cultivating computer talents, colleges and universities should ensure that students understand and deeply understand the overall computer system, master the correlation and logical level of various software or hardware parts in the computing system, and have the ability to develop and build computer systems. This paper analyzes the development characteristics of today’s computer technology, probes into the problems in cultivating computer talents in Colleges and universities, and finally explains in detail how to use technical ways to cultivate computer professional system ability, so that students can independently develop a computer system including CPU, operating system and compiler.},
  keywords={},
  doi={10.1109/ICISE-IE53922.2021.00287},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9780920,
  author={Hu, Xing and Zhang, Yang and Mao, Li and Shen, Jie and Xing, Zuocheng},
  booktitle={2021 IEEE 23rd Int Conf on High Performance Computing & Communications; 7th Int Conf on Data Science & Systems; 19th Int Conf on Smart City; 7th Int Conf on Dependability in Sensor, Cloud & Big Data Systems & Application (HPCC/DSS/SmartCity/DependSys)}, 
  title={A Noc Centric Low Overhead Multi-chip Interconnection Technology}, 
  year={2021},
  volume={},
  number={},
  pages={603-610},
  abstract={Improving the performance of processors is one of the main tasks of high-performance processor design. It is an effective way to improve the processor's performance through high-speed inter-chip interconnection, which utilizes multiple chips to build one unified computing system with higher performance. The existing multi-chip interconnection technologies, such as CCIX, CXL and NVLink, are faced with the problems of not open protocol, high hardware overhead and difficulty to reuse with existing high-speed peripherals. Considering the limitation of current multi-chip interconnection, we adopt the network centered idea to design FT_Link open architecture, which realizes the interconnection of multiple chips in a low-cost way and enables requests to access other chip spaces transparently. FT_Link system is built on common logic layer and physical layer of the existing high-speed peripherals, making it flexible and versatile. Tests and synthesis results show that FT_ Link technology has the feature of high throughput and low latency with very small hardware overhead. Its performance bottleneck depends on the FIFO depth of the high-speed peripheral logical layer which it adopts.},
  keywords={},
  doi={10.1109/HPCC-DSS-SmartCity-DependSys53884.2021.00102},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{5622752,
  author={Shufen Zhang and Xuebin Chen and Shuai Zhang},
  booktitle={2010 International Conference on Computer Application and System Modeling (ICCASM 2010)}, 
  title={The grid-based collaborative database model}, 
  year={2010},
  volume={10},
  number={},
  pages={V10-360-V10-363},
  abstract={In the collaborative grid environment, there are some characteristics of the database, such as polymorphism, self-made and distributed and so on. In general, the traditional database management system which takes information sharing as its core function cannot fully meet requirements. Therefore, it is necessary for us to establish a “collaborative management system of database”, that is, cooperative database model. This paper discusses the concept of cooperative database, establishes a collaborative database model based on MAS, and researches data query processing of cooperative database and data version management.},
  keywords={},
  doi={10.1109/ICCASM.2010.5622752},
  ISSN={2161-9077},
  month={Oct},}@INPROCEEDINGS{8766402,
  author={Krishnamurthy, Karthik and Singh, Pradeep and Sriraman, Nikhil},
  booktitle={2019 Global IoT Summit (GIoTS)}, 
  title={GeoBMS: Hybrid Cloud/ On-Premise Architecture for Building Energy Optimization}, 
  year={2019},
  volume={},
  number={},
  pages={1-6},
  abstract={Building energy optimization offers an impactful and readily addressable opportunity for reducing energy consumption in commercial buildings. Owners/operators of commercial properties (building operators) have recognized this opportunity and invested significantly in both energy management devices (such as sensors and controllers) and systems (such as Building Management Systems, BMS) to be master-controllers for these installed devices. Recent studies have revealed that an additional 10% energy savings opportunity would become available if building operators can sustain optimal BMS configurations. Unfortunately, at least three significant challenges limit ongoing BMS configuration optimization. First, most BMSs have vendor-specific interfaces that require custom, one-off integrations for remote access. Second, a lack of consistency in how building configurations and performance data is represented from one BMS to another, makes it hard to aggregate performance trends. Third, BMSs often support single users, i.e., building operators, impeding the ability to support for multi-stakeholder applications such as for personalizing occupant experiences and planning disaster evacuations. We propose a hybrid cloud/on-premise model that reuses existing BMS implementations but incorporates the benefits of new cloud technologies. Our model employs a cloud-based “middle layer” infrastructure (which we call GeoBMS) that connects on-premise BMSs to scalable building management applications. The hybrid model addresses each of the identified BMS limitations: remote inaccessibility through virtualization; inconsistent data representation through unified cloud data models; and lack of multi-stakeholder access through global authentication. GeoBMS offers published APIs for applications to access virtualized infrastructure capabilities. We present a proof-of-concept application (which we call EnergyOptimize) that uses GeoBMS to monitor energy performance and optimize building configurations. We illustrate the application using a museum case-example.},
  keywords={},
  doi={10.1109/GIOTS.2019.8766402},
  ISSN={},
  month={June},}@INPROCEEDINGS{9629212,
  author={Schoenherr, Jordan Richard and DeFalco, Jeanine},
  booktitle={2021 IEEE International Symposium on Technology and Society (ISTAS)}, 
  title={Moral education and A/IS standardization: Responsible and ethical design through education}, 
  year={2021},
  volume={},
  number={},
  pages={1-8},
  abstract={Despite years of training in computer science, there is currently no standardized, systematic integration of ethics into the curriculum. The development and communication of ethical standards represents one means to address this issue, however, in the absence of considering how information and computer science (ICS) professionals will make sense of ethical issues within a specific context, there is a growing consensus that there will be no immediate, enforceable actions that will follow ethical violations. A curriculum is needed. We consider critical features of a curriculum based on principles of moral education and review studies that have assessed aspects of an ICS ethics curriculum.},
  keywords={},
  doi={10.1109/ISTAS52410.2021.9629212},
  ISSN={2158-3412},
  month={Oct},}@INPROCEEDINGS{10187782,
  author={Plantinga, Paul and Dlamini, Nonkululeko and Pienaar, Gary and Nkosi, Moremi and Davids, Yul Derek and Okem, Andrew and Dlamini, Sifiso},
  booktitle={2023 IST-Africa Conference (IST-Africa)}, 
  title={The Governance of eParticipation: Towards a Rights-based Approach}, 
  year={2023},
  volume={},
  number={},
  pages={1-16},
  abstract={The way in which e-participation is governed is critical for ensuring inclusive outcomes. This paper presents a comparative analysis of policy, legislation and practice related to e-participation globally, and with a specific emphasis on South Africa. Our analysis reveals three distinctive features of the current e-participation governance landscape, all of which are broadly centred on a rights-based approach. Firstly, e-participation is a complement to in-person participation and should be targeted at the inclusion of people that are not normally involved in decision-making. Second, e-participation should be embedded within a wider digital rights framework which recognises possible inequities and harms resulting from the use of technology. Finally, public officials need to develop capabilities and processes for engaging with civic-led policy deliberations that take place outside of their direct control.},
  keywords={},
  doi={10.23919/IST-Africa60249.2023.10187782},
  ISSN={2576-8581},
  month={May},}@INPROCEEDINGS{8445155,
  author={Consel, Charles},
  booktitle={2018 IEEE/ACM 40th International Conference on Software Engineering: Software Engineering in Society (ICSE-SEIS)}, 
  title={Assistive Computing: A Human-Centered Approach to Developing Computing Support for Cognition}, 
  year={2018},
  volume={},
  number={},
  pages={23-32},
  abstract={The growing population of cognitively impaired individuals calls for the emergence of a research area dedicated to developing computing systems that address their needs. The nature of this research area requires to bridge the many disciplines needed to develop human-centered, assistive computing systems. Such bridging may seem unattainable considering the conceptual and practical gaps between the related disciplines and the challenges of propagating human-related concerns throughout the many stages of the development process of assistive technologies. As a consequence, existing assistive technologies lack a proper needs analysis; their development is often driven by technology concerns, resulting in ill-designed and stereotype-biased systems; and, most of them are not tested for their effectiveness in assisting users. In this paper, we propose a systematic exploration of this vast challenge. First, we define Assistive Computing as a research area and propose key principles to drive its study. Then, we introduce a tool-based methodology dedicated to developing assistive computing support, integrating a range of disciplines from human-related sciences to computer science. This methodology is purposefully pragmatic in that it leverages, aggregates and revisits numerous research results, concretizing it with a range of examples. More generally, our goal is i) to provide a framework to conduct research in the area of Assistive Computing and ii) to identify the necessary bridges between disciplines to account for all the dimensions of such systems.},
  keywords={},
  doi={},
  ISSN={},
  month={May},}@INPROCEEDINGS{10332432,
  author={Yong, Allan and Arya, Ali and Mantch, Monique},
  booktitle={2023 IEEE International Conference on Engineering, Technology and Innovation (ICE/ITMC)}, 
  title={Indigenous Technology Empowerment Model: A Community-based Design Framework}, 
  year={2023},
  volume={},
  number={},
  pages={1-9},
  abstract={This paper addresses the lack of social and cultural inclusion in the design of systems and products based on emerging technologies. Through a review of existing design processes, and focusing on Virtual Reality (VR) and Indigenous Peoples as examples, we conclude that existing design approaches for such technologies are not socially and culturally inclusive, don’t consider users as a community with its protocols and knowledge, and are not suitable for marginalized groups such as Indigenous Peoples. Through a collaborative project aiming at a prototype VR gathering place for Indigenous communities, we investigate the needs and concerns of community members and propose a framework consisting of a new information system model and its design process. We refer to this framework as Indigenous Technology Empowerment Model (ITEM), which is implemented using a hierarchical community involvement approach and elements we call Participatory Action Unit (PAU). Our prototype development follows this framework, and a qualitative analysis of community participation shows the initial success of our approach in empowering the communities to play an active role in the design process and identify the important technical features to be worked on. Our research also identifies proper community training as an essential component of a community-based design process.},
  keywords={},
  doi={10.1109/ICE/ITMC58018.2023.10332432},
