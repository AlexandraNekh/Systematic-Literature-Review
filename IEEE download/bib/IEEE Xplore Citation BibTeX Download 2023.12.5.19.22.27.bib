  booktitle={2022 Congreso Internacional de Innovación y Tendencias en Ingeniería (CONIITI)}, 
  title={Technological application in urban agriculture from a community-based approach focused on improving the quality of life of vulnerable families}, 
  year={2022},
  volume={},
  number={},
  pages={1-6},
  abstract={We present an environmentally and socially responsible project focused on empowering vulnerable communities. Particularly, we have been working with the community of Usme-UPZ57, located in Bogotá, Colombia, through the YOMASA Institutional Program. Our main objective is teaching its population how to sow and harvest their own food, not only for them to obtain an independent food supply, but in order to enable them to create a sustainable business model. A monitoring system is presented, where we propose the use of electronic devices that can record pH values, humidity, temperature, and luminosity, and visualize such measurements in an LCD screen. A detailed budget is presented, using the current prices in the market. We also present a flowchart associated with one of the variables, as well as diagrams of some circuits, including amplifiers and sensors.},
  keywords={},
  doi={10.1109/CONIITI57704.2022.9953638},
  ISSN={2539-4320},
  month={Oct},}@INPROCEEDINGS{9652331,
  author={Jara, Norma and Alarcon, Andrea and Valera, Jason},
  booktitle={2021 IEEE Sciences and Humanities International Research Conference (SHIRCON)}, 
  title={TIPI: social robot that incentives good habits to optimize learning at home for children}, 
  year={2021},
  volume={},
  number={},
  pages={1-4},
  abstract={The media has spread a notion of what robots should look like and what they should do. This document explores through a qualitative study the characteristics that Peruvian children can perceive and value of a set of robots to generate a list of design requirements. The results showed that children perceive robots as a potential companion that can be part of their daily lives, and that their ideal robot has certain characteristics that their favorite toys also possess. Tipi is a 2 DOF social robot that has an interface that is displayed on its touch screen, which allows the development and monitoring of healthy habits through interaction with the child to contribute to their personal development.},
