  booktitle={2020 International Conference on Computer Engineering and Intelligent Control (ICCEIC)}, 
  title={Research on Space Transportation Intelligence Control}, 
  year={2020},
  volume={},
  number={},
  pages={271-274},
  abstract={According to the goal of improving the ability to in and out space, artificial intelligence technology innovative application in space transportation control has been explored. First, development need and basic principle of space transportation intelligent control have been described. Secondly, development status at home and abroad has been summarized. Finally, future development direction and research focus have been proposed for rocket intelligent control, reusable rocket intelligent control and intelligent control demonstration.},
  keywords={},
  doi={10.1109/ICCEIC51584.2020.00058},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9151453,
  author={Xiang-yan, Lee and Kun-Fa, Lee},
  booktitle={2020 International Conference on Urban Engineering and Management Science (ICUEMS)}, 
  title={Research on BIM Building Space Design under the Background of Internet of Things}, 
  year={2020},
