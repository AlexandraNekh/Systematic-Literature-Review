  volume={},
  number={},
  pages={0446-0451},
  abstract={This paper considers the basic concepts of automated design and implementation of application graphical user interface supported by artificial neural networks and application of this approach to child computer interaction educational projects.},
  keywords={},
  doi={10.1109/SIBIRCON48586.2019.8958406},
