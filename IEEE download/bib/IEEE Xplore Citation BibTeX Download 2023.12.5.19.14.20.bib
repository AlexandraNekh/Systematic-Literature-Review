  author={Mou, Liqiang and Wang, Ziwen and Gu, Qin},
  booktitle={2020 2nd International Conference on Applied Machine Learning (ICAML)}, 
  title={Research on Some Key Technologies of Wireless Sensor Networks Based on Optimization Theory}, 
  year={2020},
  volume={},
  number={},
  pages={191-194},
  abstract={Wireless sensor networks automatically connect a large number of low-cost, low-power micro-sensor nodes, which can detect, monitor, collect and transmit different environmental information in the covered area in real time. Wireless sensor network has the advantages of flexible positioning, high reliability, good expansibility and good economy. Wireless sensor networks are widely used in the field of security. Military, industrial control, health and environmental monitoring, its application is more extensive. Wireless sensor networks are usually used in harsh outdoor environments or unverified areas, and the required energy is mainly provided by batteries or button batteries. In this paper, based on the optimization theory of program platform, the maximum life cycle model of multi-base station based on wireless hardware network sensor is established.},
  keywords={},
  doi={10.1109/ICAML51583.2020.00046},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{9641738,
  author={Oberoi, Aaryan and Mohan, Harini and Basavaraju, Sagar and Raparthi, Subrahmanyam and Bhattacharjee, Sourav and Cornet, Souresh},
  booktitle={2021 IEEE 9th Region 10 Humanitarian Technology Conference (R10-HTC)}, 
  title={Study of an activity tracking device for rural workers through collaborative design}, 
  year={2021},
  volume={},
  number={},
  pages={1-6},
  abstract={Majority of the rural population in India rely on agriculture as their major source of income. Due to the unforeseen circumstances and changes pertinent to season, demographics, geopolitical, etc., growth in the agricultural sector has been very constrained, thus keeping the majority below the poverty line. Mahatma Gandhi National Rural Employment Guarantee Act or MGNREGA, is an Indian labor law that strives to guarantee the ‘right to work. It is a social security measure with a goal to improve livelihood of the adults in the rural areas by providing at least 100 days of unskilled labor work. However, its efficiency is hampered by the inability to accurately track the work on a regular basis. Absence of third-party contractors may be financially beneficial, but it is causing a growing discontentment within the rural households. This paper summarizes two years of rigorous field work and articulates field experience and challenges with the MGNREGA scheme implementation in Dunda village, located in Uttarkashi district of Uttarakhand, India. The systemic and contextual causes are explored while our stay with the rural community and studied using participatory and user centric methods. This paper presents and proposes a viable solution developed using collaborative approach with the rural workers and village officials in-charge of the scheme's implementation. A wearable smart device to monitor the on-field activity of workers is proposed. A centralized worker report database to record workdays generated by each worker and task details is also proposed. Further, details about task progress and worker compensation can be regulated. It is expected that this sort of activity tracking device will improve the transparency of the process and restore confidence in the MGNREGA scheme at the grassroots.},
  keywords={},
  doi={10.1109/R10-HTC53172.2021.9641738},
  ISSN={2572-7621},
  month={Sep.},}@INPROCEEDINGS{9052293,
  author={Arce-Orozco, Armando and González-Torres, Antonio and Mata-Montero, Erick and Bener, Ayse and Jara-Watson, Hans},
  booktitle={2019 International Conference on Information Systems and Computer Science (INCISCOS)}, 
  title={A Model for Recommending Actions during the Collaborative Design of Visual Analytics Flows}, 
  year={2019},
  volume={},
  number={},
  pages={326-332},
  abstract={Visual Analytics (VA) is a process to facilitate the uncovering of knowledge from large and complex datasets to enable users to make informed decisions. However, the design of VA tools is frequently performed by multidisciplinary teams that can be physically in the same place or distributed across different locations. Therefore, there is an intrinsic collaboration need between team members and also of proposing methods to support these teams to work collaboratively during the design and implementation of VA systems. Although some proposals have made on the use of visual data flows and recommender systems, its combination with a Multi-Device Environment can benefit the discussion and exchange of knowledge during design and programming. Therefore, this article presents the design of an architecture that combines a Multi-Device Environment (MDE) with visual data flows and recommender systems for the design of VA tools in a collaborative way. The intention is to support designers and programmers during the design of visual data flows through the sharing and analysis of ideas in a collaborative environment supported by a recommender system that aids decision making during the design of flows.},
  keywords={},
  doi={10.1109/INCISCOS49368.2019.00058},
  ISSN={},
  month={Nov},}@ARTICLE{10071542,
  author={Sanderson, Conrad and Douglas, David and Lu, Qinghua and Schleiger, Emma and Whittle, Jon and Lacey, Justine and Newnham, Glenn and Hajkowicz, Stefan and Robinson, Cathy and Hansen, David},
  journal={IEEE Transactions on Technology and Society}, 
  title={AI Ethics Principles in Practice: Perspectives of Designers and Developers}, 
  year={2023},
  volume={4},
  number={2},
  pages={171-187},
  abstract={As consensus across the various published AI ethics principles is approached, a gap remains between high-level principles and practical techniques that can be readily adopted to design and develop responsible AI systems. We examine the practices and experiences of researchers and engineers from Australia’s national scientific research agency (CSIRO), who are involved in designing and developing AI systems for many application areas. Semi-structured interviews were used to examine how the practices of the participants relate to and align with a set of high-level AI ethics principles proposed by the Australian Government. The principles comprise: (1) privacy protection and security, (2) reliability and safety, (3) transparency and explainability, (4) fairness, (5) contestability, (6) accountability, (7) human-centred values, (8) human, social and environmental well-being. Discussions on the gained insights from the interviews include various tensions and trade-offs between the principles, and provide suggestions for implementing each high-level principle. We also present suggestions aiming to enhance associated support mechanisms.},
  keywords={},
  doi={10.1109/TTS.2023.3257303},
  ISSN={2637-6415},
  month={June},}@INPROCEEDINGS{6956773,
  author={Farroha, B.S. and Farroha, D.L.},
  booktitle={2014 IEEE Military Communications Conference}, 
  title={A Framework for Managing Mission Needs, Compliance, and Trust in the DevOps Environment}, 
  year={2014},
  volume={},
  number={},
  pages={288-293},
  abstract={The expanding pace of business competitiveness and increasing demand velocity for developing and deploying updated Operational and Security capabilities has created an environment where development and operations needed to work even closer together. The need was further enhanced due to the fact that all capabilities are being developed in a shared platform with no formal requirement processes, and no analysis of the overall enterprise capabilities and architecture. On the surface, the process lacks the usual discipline that most engineers are used to, but operationally it has the potential of bringing capabilities to operations at a quicker rate. The goal of providing continuously updated services should make sure that the overall enterprise performance and security posture are not compromised while the quick turnaround capability deployment is achieved. The proposed framework focuses on ensuring the continuity of strategic posturing while allowing maximum flexibility to tactical enhancements to meet emerging demands.},
  keywords={},
  doi={10.1109/MILCOM.2014.54},
