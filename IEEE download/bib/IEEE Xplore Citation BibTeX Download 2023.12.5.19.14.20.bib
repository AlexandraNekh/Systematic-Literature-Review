@INPROCEEDINGS{10296574,
  author={Durgun, Mahmut},
  booktitle={2023 Innovations in Intelligent Systems and Applications Conference (ASYU)}, 
  title={An Acoustic Bird Repellent System Leveraging Edge Computing and Machine Learning Technologies}, 
  year={2023},
  volume={},
  number={},
  pages={1-8},
  abstract={In contemporary agriculture, protecting ripening fruit crops is crucial for enhancing agricultural productivity and preventing economic losses. This study presents an intelligent, Edge Computing-based acoustic bird repellent system developed to deter birds from damaging ripening fruits in orchards. The system is equipped with microphone sensors to detect bird vocalizations and audible alarms utilized to scare off the birds. Machine learning algorithms analyze the collected audio data to identify bird sounds in real-time and generate effective bird repellent noises accordingly. This Edge Computing solution minimizes data transmission latency and network load, providing rapid and efficient protection. The system can be tailored to different bird species and optimized according to specific conditions within the orchard. Experimental tests conducted in this study demonstrate that the proposed acoustic bird repellent system is successful in safeguarding fruit crops and significantly mitigating the detrimental impact of birds. In conclusion, the Edge Computingbased acoustic bird repellent system offers a sustainable and environmentally friendly solution in the agricultural domain, assisting in the prevention of yield loss in orchards and enhancing economic benefits.},
  keywords={},
  doi={10.1109/ASYU58738.2023.10296574},
  ISSN={2770-7946},
  month={Oct},}@INPROCEEDINGS{10192628,
  author={M, Joypriyanka and R, Surendran},
  booktitle={2023 8th International Conference on Communication and Electronics Systems (ICCES)}, 
  title={VR-Game for Kids with Paraplegia using AI Agent Trained with PQR-DQN Techinique as Opponent}, 
  year={2023},
  volume={},
  number={},
  pages={741-748},
  abstract={The application of artificial intelligence (AI) and virtual reality (VR) in the medical field has demonstrated encouraging potential for enhancing the quality of life of patients who suffer from physical impairments. This comprises the creation of an artificial intelligence agent that is trained in a virtual reality game using a deep reinforcement learning (DQN) algorithm. This will be used as a form of game therapy for children who have been diagnosed with paraplegia. The virtual reality game's artificial intelligence can give the child individualized feedback based on their specific skills and how far they've come in the game. As it observes visible growth over time, this can help the child build confidence and self-esteem, which is a win-win situation. This to the enhancement of the child's confidence as well as their general well-being. Multiple studies have shown that artificial intelligence-assisted game therapy is beneficial in enhancing the ability of thinking and overall quality of life of children who have been diagnosed with paraplegia. Children who took part in a virtual reality (VR) game treatment programmed using an artificial intelligence (AI) avatar exhibited significant gains in mental ability to think and their confidence level when compared to others. The usage of a virtual reality (VR) game that incorporates an AI agent that has been trained using the DQN algorithm as a form of game therapy for children who suffer from paraplegia has the potential to significantly improve the children's overall quality of life. Researchers should anticipate seeing even more creative uses of virtual reality (VR) and Artificial Intelligence (AI) in the medical and rehabilitation fields as the technology continues to advance.},
  keywords={},
  doi={10.1109/ICCES57224.2023.10192628},
  ISSN={},
  month={June},}@ARTICLE{8960682,
  author={},
  journal={IEEE P7010/D4, January 2020}, 
