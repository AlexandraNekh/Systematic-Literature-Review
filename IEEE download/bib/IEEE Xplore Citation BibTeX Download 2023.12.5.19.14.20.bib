  doi={10.1109/CAR.2010.5456696},
  ISSN={1948-3422},
  month={March},}@INPROCEEDINGS{9845912,
  author={Lan, Guan and Yang, Yu and Wenrui, Wu and Lei, Tian and Xianxin, Li and Benliang, Li},
