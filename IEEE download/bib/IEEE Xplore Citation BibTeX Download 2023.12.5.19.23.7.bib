  booktitle={2022 4th International Conference on Pattern Analysis and Intelligent Systems (PAIS)}, 
  title={Mobile application for participatory simulation between business experts}, 
  year={2022},
  volume={},
  number={},
  pages={1-6},
  abstract={To better manage an enterprise, but above all to preserve and extend its lifespan, it is necessary to manage the skills and knowledge of experts in the field. This paper deals with this aspect by proposing a mobile application for the capitalization of business expert's knowledge in the form of business rules, which facilitates their management, in the industrial maintenance field. The proposed approach is based on the following steps: the introduction of business rules, the consistency check, the recommendation of business experts who are grouped into clusters with the unsupervised classification algorithm K-means, and finally, the automation of the negotiation phase using the extended CNP (Contract Net Protocol) protocol. The results obtained are very promising and make it possible to deal with inconsistency problems such as: invalid rules, contradiction, redundancy and rules that are never applicable.},
  keywords={},
  doi={10.1109/PAIS56586.2022.9946901},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10100268,
  author={Siddiqui, Abdulla and Chirputkar, Abhijit and Ashok, P},
  booktitle={2023 International Conference on Innovative Data Communication Technologies and Application (ICIDCA)}, 
  title={Role of Blockchain in Manufacturing and Logistics Supply Chain Management}, 
  year={2023},
  volume={},
  number={},
  pages={648-654},
  abstract={Blockchain is a distributed, decentralized ledger that is digitally managed & records transactions in an immutable format. Blockchain helps build transparency, trust, and detectability, making it an ideal fit for applications where monetary exchanges are the primary activity. Initially, the technology was developed to support transactions, but today the innovation has acquired consideration even in other areas such as retail, manufacturing, healthcare, and government services. The manufacturing industry for centuries has seen work based on their bonding and trust with their stakeholders. As regular manufacturing plants are venturing into gigafactories, the support of several middlemen will complicate the process. Subsequently, this research paper aims to outline blockchain innovation in managing the supply chain and logistics-related difficulties by figuring out and analyzing it. The audit shows how blockchain can change the logistics and supply chain network into safer, lithe, trusted, and straightforward capabilities.},
  keywords={},
  doi={10.1109/ICIDCA56705.2023.10100268},
  ISSN={},
  month={March},}@INPROCEEDINGS{1392853,
  author={Hamilton, E. and DiGiano, C. and Cole, R. and Ward, W. and LaBine, D.},
