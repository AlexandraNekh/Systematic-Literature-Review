  doi={10.1109/CCDC52312.2021.9601854},
  ISSN={1948-9447},
  month={May},}@INPROCEEDINGS{9730336,
  author={Wu, Cheng cheng and Xu, Shuhong and Lv, Yi and Wang, Lijun},
  booktitle={2022 14th International Conference on Computer Research and Development (ICCRD)}, 
  title={Evaluation System of Civil Aircraft Cockpit Design Using Mixed Reality}, 
  year={2022},
  volume={},
  number={},
  pages={349-355},
  abstract={The cockpit design of civil aircraft is about smooth operation, flight safety and flight efficiency. The dynamic evaluation of the design process is obviously conducive to the optimization of man-machine efficiency. A system design method based on mix reality is proposed, and the system design and method verification are carried out with a large-type passenger plane as the target model. The proposed method focuses on solving the dynamic matching problem of virtual and physical scenes in human in the loop, and realizes the coordinated matching between rigid simulation and flexible human body. At the same time, the problem of dynamic access of general multi-source simulation data is solved, and the coordinated dynamic visualization between numerical data and image data is realized. Verification experiment shows that the system is feasible, reliable and effective in engineering.},
