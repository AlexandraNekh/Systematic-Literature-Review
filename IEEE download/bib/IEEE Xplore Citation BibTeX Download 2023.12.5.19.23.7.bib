  volume={},
  number={},
  pages={nil13-nil52},
  abstract={},
  keywords={},
  doi={10.1109/COASE.2007.4341642},
  ISSN={2161-8089},
  month={Sep.},}@INPROCEEDINGS{10303349,
  author={Li, Jia-Rong and Huang, Hsin-Yi and Chang, Teng-Wen and Shih, Chi-Chi and Chien, Hsiang-Ting},
  booktitle={2023 27th International Conference Information Visualisation (IV)}, 
  title={Constructing a Cross-Disciplinary Idea Convergence System Using AIGC: A Case Study of Engineering and Design}, 
  year={2023},
  volume={},
  number={},
  pages={352-357},
  abstract={In response to globalization and increased competition, individual professionals in a single field often cannot provide comprehensive solutions. Collaboration among professionals from different disciplines is required to find more effective solutions. This study aims to develop a system called AIGC (AI Generated Content) to facilitate idea convergence in interdisciplinary teams. The system aims to address the collaboration challenges between engineering and design fields and facilitate consensus through the introduction of images and visual elements. To achieve this goal, qualitative research methods were employed, including contextual interviews, the KJ method, brainstorming, personas, and user journey mapping, among others. These methods were chosen because they provide in-depth understanding of communication and collaboration patterns among participants and offer valuable insights. The selection of participants was based on their experience and expertise in interdisciplinary projects. Students from design and engineering fields were chosen as research participants to ensure diversity and representativeness. During the execution of the study, the research team collected data through contextual interviews and related qualitative research methods. The steps involved in contextual interviews included selecting appropriate scenarios, designing relevant guided questions, and conducting face-to-face interviews. Through these methods, we gained deep insights into participants' communication patterns, thought processes, and collaboration needs. The results of this study demonstrate that the AIGC system, which facilitates interdisciplinary idea convergence, effectively promotes consensus among participants. This is achieved through interactive processes such as selecting images, describing images, and engaging in interactive design. The application of the system is significant in addressing collaboration challenges between the engineering and design fields and holds potential value in enhancing teamwork and creative output.},
  keywords={},
  doi={10.1109/IV60283.2023.00066},
  ISSN={2375-0138},
  month={July},}@INPROCEEDINGS{9422521,
  author={Watkins, Lanier and Cho, Cheng-Hao and Hurley, John and Rubin, Aviel},
  booktitle={2021 IEEE International IOT, Electronics and Mechatronics Conference (IEMTRONICS)}, 
  title={Collaborative Global Impact Cloud Computing Risk Assessment Framework}, 
  year={2021},
  volume={},
  number={},
  pages={1-5},
  abstract={Currently, risk assessment is more of an art than a science. In this paper, we demonstrate how a previously proposed theoretical risk assessment framework that focuses on making risk assessment more scientific can be implemented and applied to cloud networks. Our work offers a practical risk assessment implementation for cloud networks where disparate network owners can directly measure network risks in an objective, uniform and repeatable manner across networks by allowing the network owners to collaboratively agree on risk metrics and continuously monitor their cloud networks with the same tool, which employs these agreed upon metrics. The end result is a science-based risk metric (SBRM) based on the global impact of the vulnerabilities in the network. Specifically, we demonstrate the feasibility of implementing this SBRM by: (1) imposing real vulnerabilities on realistic cloud networks built from real hardware (multi-core 3 Ghz Xeon Deterlab servers), (2) demonstrating the operational use of our risk assessment implementation to measure risk across several emulated cloud networks, and (3) describing our approach.},
  keywords={},
  doi={10.1109/IEMTRONICS52119.2021.9422521},
  ISSN={},
  month={April},}@INPROCEEDINGS{10144161,
  author={Ludlow, David and Khan, Zaheer and Chrysoulakis, Nektarios and Mitraka, Zina},
  booktitle={2023 Joint Urban Remote Sensing Event (JURSE)}, 
  title={Towards Digital-Twin solutions for the 15 minute city}, 
  year={2023},
  volume={},
  number={},
  pages={1-4},
  abstract={The Covid-19 pandemic and the climate emergency have re-emphasised the need to re-evaluate urban governance and planning process in cities. The "new-normal" and digital transformations offer catalysts to drive and deliver climate mitigation in cities. However, mitigation solutions such as liveable neighbourhoods or 15-minute cities require data-driven intelligence for integrated assessment of alternative mitigation strategies. This paper considers the effectiveness of Digital-Twin solutions in making these integrated assessments supporting the delivery of "new-normal" potentials. It also assesses the extent to which existing technological solutions like Copernicus cross-cutting applications can aid these urban transformations.},
  keywords={},
  doi={10.1109/JURSE57346.2023.10144161},
  ISSN={2642-9535},
  month={May},}@INPROCEEDINGS{9669898,
  author={Trabucco, Juan Trelles and Li, Pengyuan and Arighi, Cecilia and Raciti, Daniela and Shatkay, Hagit and Marai, G. Elisabeta},
  booktitle={2021 IEEE International Conference on Bioinformatics and Biomedicine (BIBM)}, 
  title={ANIMO: Annotation of Biomed Image Modalities}, 
  year={2021},
  volume={},
  number={},
  pages={1069-1076},
  abstract={Figures within biomedical articles present essential evidence of the relevance of a publication in a curation workflow. In particular, visual cues of the image modality or experimental methods can help expert curators identify relevant papers from an increasing number of publications. Automating the identification of these content-bearing images can thus be helpful in computer-assisted curation. However, the paucity of labeled datasets and the specialized training required to label such images hinder the development of such tools. To address this problem, we present the design of ANIMO, a labeling system that integrates extraction and segmentation tools to ease the annotation burden. We first introduce two taxonomies of image modalities and experimental methods, derived in collaboration with curators. On the back-end of the system, we process batches of documents and create a labeling task per document. At the front-end, expert curators can access these tasks through a web interface and access the article of interest. We describe the evaluation of this system by a group of biocurators, and the human factor lessons learned from this interdisciplinary experience.},
  keywords={},
  doi={10.1109/BIBM52615.2021.9669898},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{6403211,
  author={Lorenzo, Carlos-Miguel and Lezcano, Leonardo and Vecino, Carlos and Sicilia, Miguel-Ángel},
  booktitle={2012 International Symposium on Computers in Education (SIIE)}, 
  title={SLRoute: Learning Spanish in immersive environments through the Way of St. James}, 
  year={2012},
  volume={},
  number={},
  pages={1-5},
  abstract={We are currently experiencing a boom in the presence of Spanish language in the world, which is reflected in its inclusion as a second language in the educational system of countries like Brazil and the emergence of language in the U.S. and China. To confront this situation there is a wide variety of courses for learning Spanish. However, specific initiatives combining proven teaching methods in university classroom experiences with the creation of new multidisciplinary content displayed on 3D virtual sets have not been detected. The objective of this research, supported by the SLROUTE project, is the creation of an integrated technology platform that enables the creation, development and deployment of contents for teaching Spanish in an educational video game. Such environment will promote an immersive, creative and collaborative experience in the process of learning Spanish.},
  keywords={},
  doi={},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{9845511,
  author={Thiga, Moses and Kimeto, Pamela and Mgala, Mvurya and Kweyu, Emmanuel and Wanyee, Steve and Mwirigi, Tooroti},
  booktitle={2022 IST-Africa Conference (IST-Africa)}, 
  title={A Remote Blood Pressure Data Collection and Monitoring System for Expectant Mothers}, 
  year={2022},
  volume={},
  number={},
  pages={1-9},
  abstract={Pre-enclampsia, a condition evidenced by persistent high blood pressure during pregnancy, can lead to the loss of both the mother and child, and often times persists beyond the delivery of the baby. Its early detection through regular blood pressure measurements can inform timely and life saving interventions for both the mother and baby. This study developed a system for remote blood pressure data collection and monitoring incorporating (i) a smartwatch with Photolethysmography (PPG) and Electrocardiogram (ECG) sensors, (ii) a mobile application for receiving the readings through bluetooth and (iii) a mobile application for use by caregivers, namely; ante natal clinic nurses, community health extension workers, community health workers and next of kin, to monitor the blood pressure readings for mothers assigned to them. The system demonstrates significant potential in the early detection of pre-eclampsia, which will in turn inform timely interventions to prevent fatal complications for both mother and baby.},
  keywords={},
  doi={10.23919/IST-Africa56635.2022.9845511},
  ISSN={2576-8581},
  month={May},}@INPROCEEDINGS{4281417,
  author={Ji, Shuyan and Robotham, Tony and Roberts, Ibiye},
  booktitle={2007 11th International Conference on Computer Supported Cooperative Work in Design}, 
  title={Problem Solving Facilities in Human-Centred Collaborative Environment}, 
  year={2007},
  volume={},
  number={},
  pages={98-103},
  abstract={SMEs suffer at times through lack of expertise and knowledge and do not always make full use of their people by converting tacit knowledge to explicit product design work. They may need to collaborate with multiple experts from different disciplines through an on-line virtual collaborative workspace to implement a complicated product design. This paper presents a Web-based collaborative working environment to help SME customers to leverage all the interests from different experts in order to obtain design optimisation. The system provides problem-solving facilities including distributed multiple objective design optimization tool and support users to dynamically define design objectives, design complaints and design variables.},
  keywords={},
  doi={10.1109/CSCWD.2007.4281417},
  ISSN={},
  month={April},}@INPROCEEDINGS{9041799,
  author={Laure, Erwin and Eriksson, Olivia and Lindahl, Erik and Henningson, Dan},
  booktitle={2019 15th International Conference on eScience (eScience)}, 
  title={The Future of Swedish e-Science: SeRC 2.0}, 
  year={2019},
  volume={},
  number={},
  pages={413-420},
  abstract={Since 2010, the Swedish e-Science Research Centre (SeRC) is funding and coordinating e-Science activities in a broad spectrum of scientific disciplines. After an initial 5-year phase that produced outstanding results, SeRC is increasingly focusing on fostering interactions between disciplines and has created so-called Multidisciplinary Collaborative Programs (MCPs). In these programs, domain researchers collaborate with e-Science methods and tool developers and e-Infrastructure providers. In this paper we give an overview of the initial phase of SeRC and present the new programs that started operating in 2019.},
  keywords={},
  doi={10.1109/eScience.2019.00053},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10253030,
  author={Umar, Muhammad Aminu and Lano, Kevin},
  booktitle={2023 3rd International Conference on Electrical, Computer, Communications and Mechatronics Engineering (ICECCME)}, 
  title={Automated Requirements Engineering in Agile Development: A Practitioners Survey}, 
  year={2023},
  volume={},
  number={},
  pages={1-7},
  abstract={Requirements Engineering (RE) in the context of agile development is the process of discovering, analysing, validating, and managing software requirements through engaging with the stakeholders. RE activities are central to software development and its success. Advances in automation have been explored in requirements engineering activities and processes in the industry. However, the application of automated support is still limited in practice. In this study, we examine automated RE support in agile development through a qualitative analysis using industry experts. We have found that the application of automated RE support tools is primarily in the experimental stage, largely due to the limited practical implementation in industry settings. The current focus has been on research experiments, with relatively fewer instances of real-world industry application. Although experts are optimistic about its huge potential to reduce development efforts and costs if automation is harnessed. Therefore, we anticipate that these findings will be beneficial for practitioners responsible for developing RE tools for agile development, as well as for the researchers.},
  keywords={},
  doi={10.1109/ICECCME57830.2023.10253030},
  ISSN={},
  month={July},}@INPROCEEDINGS{9962671,
  author={Khoukhi, Amar},
  booktitle={2022 IEEE Frontiers in Education Conference (FIE)}, 
  title={Collaborative Project-Based Training and Learning Through Multidisciplinary Undergraduate Research}, 
  year={2022},
  volume={},
  number={},
  pages={1-5},
  abstract={This Innovate Practice Work In Progress paper presents an integrated approach into a course-based STEM undergraduate research experience. The objective of the course is to provide students with proven hands-on experience that teaches integration of discipline-specific knowledge with engineering practice on challenging multidisciplinary projects. The course design, instruction delivery, project preparation, team formation, system, and sub-system construction, as well as student and course assessment will be discussed and faced challenges emphasized. Initial feedback from students and faculty shows that the benefits include increasing interaction with the faculty and increasing intellectual maturity, knowledge, and confidence for the students. As for the faculty, this includes furthering of research projects and achieving some of program objectives as required by accreditation boards.},
  keywords={},
  doi={10.1109/FIE56618.2022.9962671},
  ISSN={2377-634X},
  month={Oct},}@INPROCEEDINGS{9582502,
  author={Delabeye, Romain and Penas, Olivia and Ghienne, Martin and Kosecki, Arkadiusz and Dion, Jean-Luc},
  booktitle={2021 IEEE International Symposium on Systems Engineering (ISSE)}, 
  title={MBSE analysis for energy sustainability improvement in manufacturing industry}, 
  year={2021},
  volume={},
  number={},
  pages={1-8},
  abstract={With the ever increasing complexity of Industry 4.0 systems, plant energy management systems developed to improve energy sustainability become equally complex. Based on a Model-Based Systems Engineering analysis, this paper aims to provide a general approach to perform holistic development of an autonomous energy management system for manufacturing industries. This Energy Management System (EMS) will be capable of continuously improving its ability to assess, predict, and act, in order to improve by monitoring and controlling the energy sustainability of manufacturing systems. The approach was implemented with the System Modeling Language (SysML).},
  keywords={},
  doi={10.1109/ISSE51541.2021.9582502},
  ISSN={2687-8828},
  month={Sep.},}@INPROCEEDINGS{10045296,
  author={Valencia, Stephanie and Luria, Michal and Pavel, Amy and Bigham, Jefrey P. and Admoni, Henny},
  booktitle={2021 16th ACM/IEEE International Conference on Human-Robot Interaction (HRI)}, 
  title={Co-designing Socially Assistive Sidekicks for Motion-based AAC}, 
  year={2021},
  volume={},
  number={},
  pages={24-33},
  abstract={Augmentative and alternative communication (AAC) devices enable speech-based communication. However, AAC devices do not support nonverbal communication, which allows people to take turns, regulate conversation dynamics, and express intentions. Nonverbal communication requires motion, which is often challenging for AAC users to produce due to motor constraints. In this work, we explore how socially assistive robots, framed as “sidekicks,” might provide augmented communicators (ACs) with a nonverbal channel of communication to support their conversational goals. We developed and conducted an accessible co-design workshop that involved two ACs, their caregivers, and three motion experts. We identified goals for conversational support, co-designed prototypes depicting possible sidekick forms, and enacted different sidekick motions and behaviors to achieve speakers’ goals. We contribute guidelines for designing sidekicks that support ACs according to three key parameters: attention, precision, and timing. We show how these parameters manifest in appearance and behavior and how they can guide future designs for augmented nonverbal communication.},
  keywords={},
  doi={},
  ISSN={2167-2148},
  month={March},}@INPROCEEDINGS{10078936,
  author={Yue, Guangpeng and Yin, Hang},
  booktitle={2022 2nd International Conference on Networking, Communications and Information Technology (NetCIT)}, 
  title={Research on Concurrent Design Method of Electronic Products and Virtual Prototype Simulation Environment Based on Virtual Reality}, 
  year={2022},
  volume={},
  number={},
  pages={371-374},
  abstract={With the gradual transformation of science and technology to productivity, the design of mechatronics products has involved many fields such as machinery, electricity and control. “Virtual reality” is no stranger to most people these days. Although virtual reality is just emerging, the magic it shows is impressive. It can create various dream realms and real scenes, which is a brand-new form of expression and has gradually become a hot topic of public discussion. In fact, what we often call “VR” is virtual reality, which is a cutting-edge technology that has rapidly penetrated into various fields. In recent years, virtual reality has developed rapidly and become a new “tuyere”, but virtual reality is still in the sunrise period, not very mature, and there are still many problems to be solved. Virtual prototype is the integration of multi domain digital models of products, which reflects the essential characteristics of products. Virtual prototyping technology is an advanced digital design method based on concurrent engineering, CAx / DFX technology and collaborative simulation technology. From the perspective of current production demand, mechatronics must constantly adopt advanced technical means in order to maximize the system efficiency.},
  keywords={},
  doi={10.1109/NetCIT57419.2022.00094},
  ISSN={},
  month={Dec},}@ARTICLE{9873879,
  author={},
  journal={ISO/IEC/IEEE P24748-2/CD, August 2022}, 
  title={ISO/IEC/IEEE Draft International Standard - Systems and software engineering -Life cycle management - Part 2: Guidelines for the application of ISO/IEC/IEEE 15288 (System life cycle processes)}, 
  year={2022},
  volume={},
  number={},
  pages={1-71},
  abstract={ISO/IEC/IEEE 15288 establishes a common framework for system life cycle processes, with well-defined terminology, that can be referenced by any party, particularly those having any involvement or interest in the system life cycle or performance of systems engineering activities. It applies to the acquisition of systems, which can be comprised of products, services, or both, as well as to the supply, development, operation, maintenance, and disposal of systems, whether performed internally or externally to an organization. ISO/IEC/IEEE 15288 may be used stand-alone or jointly with other International Standards, such as ISO/IEC/IEEE 12207, and supplies a process reference model that supports process capability assessment according to ISO/IEC 33002.},
  keywords={},
  doi={},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{9914080,
  author={Saxena, Niti and Chakravarthi, Dhruva Sreenivasa and Venkatesh, A. Narasima and Soni, Nupur and Kant, Shashi},
  booktitle={2022 International Conference on Innovative Computing, Intelligent Communication and Smart Electrical Systems (ICSES)}, 
  title={The Future of Blockchain Technology and the Internet of Things in Healthcare}, 
  year={2022},
  volume={},
  number={},
  pages={1-9},
  abstract={Using body sensor networks and other wearable sensors to practise for emergencies is becoming more and more common. If privacy and safety of patients are protected in healthcare systems, then using technology may be helpful. Block chain technology has quickly become a popular research topic on websites like Wikipedia and Google. We look at developments and ideas in the Internet of Things that can be used in the vast majority of situations. The healthcare business has a lot of good reasons to use the Block chain. It has everything that people want: decentralisation, immutability, security, privacy, and being open to everyone. The vast majority of the research is focused on these parts. As part of a plan to improve the efficiency of health care services, the Internet of Things (IoT) and other cutting-edge computer technologies were used. In the next section, the application is explained in great detail. It's no secret that medical professionals are looking into the potential of the Internet of Things (IoT) and Block Chain technology in the areas of medication tracking, remote patient monitoring, storing and retrieving medical data, and managing patient medical records. Also, it's important to note that the challenges of using IoT and blockchain technologies in healthcare systems have been well researched and are now being talked about.},
  keywords={},
  doi={10.1109/ICSES55317.2022.9914080},
  ISSN={},
  month={July},}@INPROCEEDINGS{10007315,
  author={S, Manish Venkat and Mastan Rao, Parimi and Babu, Shekar},
  booktitle={2022 3rd International Conference on Computing, Analytics and Networks (ICAN)}, 
  title={Evaluating Social Responsible Attitudes and Opinions using Sentiment Analysis – An Indian Sentiment}, 
  year={2022},
  volume={},
  number={},
  pages={1-7},
  abstract={Most companies in India who are under the ambit of Companies Act 2013 are mandated to participate, communicate, and create social impact at the bottom of the pyramid, through Corporate Social Responsibility (CSR). Corporate Social Responsibility (CSR) has also gained prominence in academic research through several challenges exposed due to the mandated CSR in India. There are already several research studies were conducted to understand various aspects of CSR. Researchers in some studies carried out research with the help of methods and techniques of qualitative analysis from the data collected from traditional sources of communication like print media and electronic media. In recent times, social media has emerged as a compelling communication platform with its remarkable contribution to providing a haven for its users to express their freedom of speech. However, academic literature still lacks empirical research that show how companies in India are performing through social media communication. Among all the available social media platforms, Twitter is popular globally and most dominant for communication and impact. Also, since Twitter is widely used by all layers of society, from the most influential people and the most powerful organizations to the common person, and the tweets contain massive data. In this paper, the authors have used sentiment analysis for evaluating the communication of the Indian companies of Indian. The study is based on a sample of 9,000 plus Tweets during a month. Using the cloud based and sentiment analysis the authors have analyzed the communication of these companies.},
  keywords={},
  doi={10.1109/ICAN56228.2022.10007315},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9479675,
  author={Jie, Hu and Zhigang, Gao Fang},
  booktitle={2021 2nd International Conference on Education, Knowledge and Information Management (ICEKIM)}, 
  title={Analysis on the Construction Method of Computer Aided Modeling Design Support System}, 
  year={2021},
  volume={},
  number={},
  pages={624-627},
  abstract={Computer aided modeling design is a creative activity that takes products as the main object., and uses computers to comprehensively create and design the functions., structures., materials., processes., visual transmission., aesthetic basis and market relations related to modeling., so that it can obtain the coordination and unity of man., machine and environment and meet the requirements of the times. This paper first expounds the present situation of CAID., then analyzes the concrete application of computer in product modeling design and expounds the general design process and product image design of computer aided modeling in detail; finally., it analyzes the development trend of computer aided conceptual design., including the development trend of computer aided design., the key technical problems of computer aided design and the development direction of modern product design modeling., in order to make people pay more attention to the computer aided application in product design. Computer aided modeling design is an important means in product design., which can improve the efficiency of modeling design and is indispensable in modeling design and system construction.},
  keywords={},
  doi={10.1109/ICEKIM52309.2021.00141},
  ISSN={},
  month={Jan},}@INPROCEEDINGS{9995916,
  author={Ghazal, Taher M. and Hasan, Mohammad Kamrul and Abdul Wahab, Amelia Natasya and Ibrahim, Amer and Khan, Wasim Ahmad and Raza, Neha Zulfiqar and Atta, Ayesha and Mago, Beenu},
  booktitle={2022 International Conference on Cyber Resilience (ICCR)}, 
  title={Towards Privacy Provisioning for Internet of Things (IoT)}, 
  year={2022},
  volume={},
  number={},
  pages={01-07},
  abstract={Internet of things (IoT) providing an environment to connect all the things from the world to internet. It interconnects smart things and provides advanced building management systems, smart city services, participatory sensing applications which improve every day's life. IoT hold wide range of physical objects embedded with tags, sensors, devices that had not been involved with internet and allow them to interact to provide IoT applications. The adoption of IoT is ubiquitous but it also arises the major concerns about “Privacy”. Privacy includes the secretion of personal information as well as therapy of data. This paper accommodates systematic literature review of privacy threats and challenges along with the techniques used to deal and address them which emerged in the research literature. I observed that very rare models consented to eliminate privacy aspects. I also analyzed that the focus of the multiple solution providers was user-centric point of view. Not one of the existing surveys analyze the privacy of IoT in terms of organized analysis of the privacy fundamentals and approaches. There is need to work on it.},
  keywords={},
  doi={10.1109/ICCR56254.2022.9995916},
  ISSN={},
  month={Oct},}@ARTICLE{10098162,
  author={Eaton, Casey E. and Mesmer, Bryan},
  journal={IEEE Open Journal of Systems Engineering}, 
  title={The Relationship Between Goal-Function Trees and Value Models}, 
  year={2023},
  volume={1},
  number={},
  pages={94-110},
  abstract={Systems often inspire the development of new models to manage size and complexity challenges in system development. Engineers should be aware of overlap in the models used for systems engineering of systems as it would be inefficient to reiterate overlapping tasks. The objective of this article is to identify consistencies between two models that appear to have overlapping tasks: Goal-Function Trees (GFTs) and value models. Both models are perceived to represent information about the system and stakeholder preferences, but have historically used differing processes and language. An analysis of the functional use and terminology for the two models reveals consistencies. The two models are compared in this article using content analysis of systems engineering academic literature. It is seen that physics-based system representation and stakeholder preference incorporation are often common to both models. A coffee maker is used to illustrate the potential consistencies and overlapping tasks between the models. Extending this simple example, the applicability of the models to space systems is discussed. The findings of this article can be used by systems engineers to improve their efficiency when applying both GFTs and value models.},
  keywords={},
