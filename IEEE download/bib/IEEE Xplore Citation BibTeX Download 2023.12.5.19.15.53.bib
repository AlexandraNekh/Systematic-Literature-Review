  doi={10.1109/TVCG.2023.3247065},
  ISSN={1941-0506},
  month={May},}@INPROCEEDINGS{10045095,
  author={Alves-Oliveira, Patrícia and Arriaga, Patrícia and Paiva, Ana and Hofman, Guy},
