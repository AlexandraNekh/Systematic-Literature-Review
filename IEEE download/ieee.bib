  ISSN={},
  month={March},}@INPROCEEDINGS{9026052,
  author={Langen, Manfred},
  booktitle={2016 International Conference on Engineering, Technology and Innovation/IEEE lnternational Technology Management Conference (ICE/ITMC)},
