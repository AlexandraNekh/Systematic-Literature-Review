  month={Oct},}@INPROCEEDINGS{10284943,
  author={Muchinenyika, Simon Hlekisana and Shava, Fungai Bhunu and Bastos, João Baptista},
  booktitle={2023 International Conference on Emerging Trends in Networks and Computer Communications (ETNCC)},
  title={A Secure Cardless Automated Teller Machine},
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={Automated Teller Machines (ATM) are critical to banking operations due to their convenient and widespread use. Apart from queues reduced in banking halls, bank clients can still access services outside the regular operational hours. However, a rise in ATM fraud cases has led to banks improving ATM security by adding security ATM keypad covers, surveillance cameras, and even ATM security booths, measures that were not there at ATMs' inception. The academia is also replete with literature on various ATM designs to enhance the security of the facility and the user. However, proposed solutions rarely build on existing infrastructure and technologies, making them difficult to implement as they require additional equipment. For IoT-based solutions, testing is relegated to the communication of components without evaluating the practicability of the proposed solution. In an effort to correct these challenges and secure an ATM user, this paper introduces a conceptual design of a secure cardless ATM system that leverages existing technologies and infrastructure with little additional costs for implementation. A workshop was then used to identify improvements for future implementation. Recommendations from stakeholders and lessons learned from the workshop are discussed.},
  keywords={},
  doi={10.1109/ETNCC59188.2023.10284943},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{9524861,
