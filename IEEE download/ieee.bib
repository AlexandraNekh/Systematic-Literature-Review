  booktitle={6th Iberian Conference on Information Systems and Technologies (CISTI 2011)},
  title={Math4Kids},
  year={2011},
  volume={},
  number={},
  pages={1-6},
  abstract={Using the notion that children learn by interacting with the surrounding world and by playing, the project sets an innovative approach and environment to provide mathematical concepts to children in the ages from 5 to 7 through interaction with a computer game. Notions such as numbers, weight, dynamics, gravity, collision, geometry, adding, sets and groups are used in a fun problem-solving oriented approach, promoting and stimulating the acquisition and learning process of the basics of Mathematics, the game being used as an auxiliary tool in pre-school classes.},
  keywords={},
  doi={},
  ISSN={2166-0735},
  month={June},}@INPROCEEDINGS{10303457,
  author={Chang, Teng-Wen and Shih, Chi-Chi and Chien, Hsiang-Ting and Tsai, Shih-Ting and Chang, Hsu-Feng and Chen, He-Chin},
  booktitle={2023 27th International Conference Information Visualisation (IV)},
  title={Discussion on Preliminary Digital Assistance Mode in the Empathy Game Process of SPRINT Warm-Up},
  year={2023},
  volume={},
  number={},
  pages={368-373},
  abstract={This research introduces the interactive process design of an Ice Breaking Stage. The Ice Breaking Stage serves as the starting point for participants in cross-disciplinary teams to engage in an Empathy game. The purpose of this stage is to allow team members from different disciplines to understand each other's personality traits. Thoughts, and perspectives through interactive activities and collaborative tasks. Based on the literature review, a conceptualized interactive process can be designed using visualizations, problem prompts, role-playing, and scenario simulations. The steps involved in the Ice Breaking Stage design include role review, initial sharing, system assistance, sharing created scenarios, simulating role-playing, and sharing solutions. All are aimed at guiding participants into a simulated role-playing scenario. Through user testing and preliminary research analysis, this study explores the digital-assisted interactive design method of the Ice Breaking Stage, focusing on sustainable development. The method utilizes genetic algorithms to generate virtual creatures with different body parts, enhancing knowledge sharing and the introductory process among team members. The research outcomes include the generation of virtual dynamic creatures using square and spherical shapes, with observed smooth dynamic operations. However, the combination of square-shaped creatures showed inconsistency, occasional movement pauses, and collision issues. On the other hand, the combination of spherical creatures visually presented smoother results. Additionally, user interface and derivative designs to generate virtual dynamic creatures can assist users in creating different contextual combinations. This approach is feasible. Future directions for exploration may include considering selection patterns and expanding the combination models of creatures and body parts. Adjusting variations in geometric shapes, size controls, and interface visual displays.},
  keywords={},
  doi={10.1109/IV60283.2023.00069},
  ISSN={2375-0138},
  month={July},}@INPROCEEDINGS{10105406,
  author={Liu, Yongxiao},
  booktitle={2023 IEEE 6th Eurasian Conference on Educational Innovation (ECEI)},
  title={Application Scenarios and Patterns of Virtual Digital VR Technology in Environmental Art Design},
  year={2023},
  volume={},
  number={},
  pages={51-54},
  abstract={Present environmental art design cannot make users feel the true and clear environmental art design effects. Therefore, the application of three-dimensional virtual VR technology in environmental art design is studied. By analyzing the application of three-dimensional virtual VR technology in environmental art design, a series of operations such as modeling types, components, terrain models, and real three-dimensional models are implemented. The application of three-dimensional virtual VR technology in urban garden environmental art design is analyzed in terms of the specific application of traffic environment art design and community environment art design.},
  keywords={},
  doi={10.1109/ECEI57668.2023.10105406},
  ISSN={},
