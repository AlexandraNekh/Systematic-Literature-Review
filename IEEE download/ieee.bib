  title={Research on Space Transportation Intelligence Control},
  year={2020},
  volume={},
  number={},
  pages={271-274},
  abstract={According to the goal of improving the ability to in and out space, artificial intelligence technology innovative application in space transportation control has been explored. First, development need and basic principle of space transportation intelligent control have been described. Secondly, development status at home and abroad has been summarized. Finally, future development direction and research focus have been proposed for rocket intelligent control, reusable rocket intelligent control and intelligent control demonstration.},
  keywords={},
  doi={10.1109/ICCEIC51584.2020.00058},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9151453,
  author={Xiang-yan, Lee and Kun-Fa, Lee},
  booktitle={2020 International Conference on Urban Engineering and Management Science (ICUEMS)},
  title={Research on BIM Building Space Design under the Background of Internet of Things},
  year={2020},
  volume={},
  number={},
  pages={100-104},
  abstract={Nowadays, BIM technology has set off a trend of research and practical application of this technology worldwide. Information technology represents one of the emerging models for the construction sector, is affecting the development of the construction industry, coupled with efficient networking technology to collect information is gradually recognized by the public, and BIM technology information through digital simulation building has real information coincide. The combination of these two technologies looks forward to the future development trend of BIM building space in the direction of artificial intelligence design.},
  keywords={},
  doi={10.1109/ICUEMS50872.2020.00032},
  ISSN={},
  month={April},}@INPROCEEDINGS{9437812,
  author={Xu, Qingyu and Sun, Yuling and He, Liang},
  booktitle={2021 IEEE 24th International Conference on Computer Supported Cooperative Work in Design (CSCWD)},
  title={Design Considerations for Information Collaboration in Long-term Senior Care System},
  year={2021},
  volume={},
  number={},
  pages={1040-1045},
  abstract={In CSCW and related fields, ICT supported collaborative design has been used in various fields. Yet, there has been little research focusing on the information collaboration between medical care and non-medical care in long-term senior caring settings. For filling this gap, this paper explores the information collaboration and communication process between formal medical carers and informal nonmedical carers in the process of long-term senior care in more depth. We report our findings from three dimensions: self-awareness of different stakeholders, their informative collaborative practices, and knowledge gap among them. Based on our findings, we proposed design considerations that combines the concept of peer to peer and machine learning algorithms, which provides alternative ideas for future design.},
  keywords={},
  doi={10.1109/CSCWD49262.2021.9437812},
  ISSN={},
  month={May},}@INPROCEEDINGS{5380960,
