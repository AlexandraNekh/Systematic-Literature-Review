  author={Reiche, Finn and Timinger, Holger},
  booktitle={2021 IEEE Technology & Engineering Management Conference - Europe (TEMSCON-EUR)},
  title={Process Model for Integrated Product Lifecycles Using Digital Twins and Predictive Analytics},
  year={2021},
  volume={},
  number={},
  pages={1-5},
  abstract={Digital development processes and methods are already established in software development. They promise speed, more flexibility in case of changes and thus customer-oriented products. In mechanical and plant engineering, as well as in mechanical products in general, these methods and this approach are not yet common. With our process model, we show how digitalisation can bring these development processes and methods. Therefore, a process model based on DevOps, known from software development, was created to allow iterative and digital engineering in hardware design. According to a case study concerned, this process model allows to accept changes during development at a later point in time and enable iterative testing. The concept has to be proven in practice in a planned development project based on this method.},
  keywords={},
  doi={10.1109/TEMSCON-EUR52034.2021.9488653},
  ISSN={},
  month={May},}@ARTICLE{10175526,
  author={Jadhav, Akshay and Shandilya, Shishir Kumar and Izonin, Ivan and Gregus, Michal},
  journal={IEEE Access},
  title={Effective Software Effort Estimation Leveraging Machine Learning for Digital Transformation},
  year={2023},
  volume={11},
  number={},
  pages={83523-83536},
  abstract={Software effort estimation is a necessary component of software development projects that belong to industrial software systems and digital transformation initiatives. Digital transformation refers to the process of integrating digital technology into various components of a company or organization in order to improve operations, procedures, customer experiences, and overall performance. Industrial software systems are trained software packages designed for use in industrial and manufacturing processes. The paper deals with the machine learning based effort estimation in order to create an effective and robust model for predicting effort. The paper proposes an Omni-Ensemble Learning (OEL) approach, which is a combination of static ensemble selection along with genetic algorithm and dynamic ensemble selection. The paper identifies the impact of software effort estimation in industrial software system, and works on the these attributes to implement a robust ensemble model. The proposed Omni-Ensemble Selection (OES) provides better overall performance (in terms of evaluation metrics) and on comparing with multiple machine learning models over Finnish and Maxwell datasets.},
  keywords={},
  doi={10.1109/ACCESS.2023.3293432},
  ISSN={2169-3536},
  month={},}@INPROCEEDINGS{5375178,
  author={Li-rong Wang and Yu Bo and Ichiro Hagiwara},
  booktitle={2009 IEEE 10th International Conference on Computer-Aided Industrial Design & Conceptual Design},
  title={Mobile-C based agent management for collaborative visualization of distributed mesh model},
  year={2009},
  volume={},
  number={},
  pages={1127-1132},
  abstract={Collaborative working during product design is highly desired to the modern globalized industry. Multi-agent system (MAS) technology is widely used to manage artificial intelligence of distributed system. Effective mechanism for self-organization of agents is important to MAS system. In this paper, a mobile-C based agent management is introduced and applied into a collaborative visualization of product mesh models among distributed designers. This collaborative visualization environment helps to realize information exchange and cooperative visualization of product model during complex concurrent processes. A prototype is developed through a collaborative mesh simplification process.},
  keywords={},
  doi={10.1109/CAIDCD.2009.5375178},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9222896,
  author={Miller, Gloria J.},
  booktitle={2020 15th Conference on Computer Science and Information Systems (FedCSIS)},
  title={Digital assets for project-based studies and data-driven project management},
  year={2020},
  volume={},
  number={},
  pages={591-594},
  abstract={Projects offer learning opportunities and digital data that can be analyzed through a multitude of theoretical lenses. They are key vehicles for economic and social action, and they are also a primary source of innovation, research, and organizational change. This research involves a survey of digital assets available through a project; specifically, it identifies sources of data that can be used for practicing data-driven, context-specific project management, or for project-based academic research. It identified four categories of data sources – communications, reports/records, model representations, and computer systems – and 51 digital assets. The list of digital assets can be inputs in the creation of project artifacts and sources for monitoring and controlling project activities and for sensemaking in retrospectives or lessons learned. Moreover, this categorization is useful for decision support and artificial intelligence systems model development that requires real-world data.},
  keywords={},
  doi={10.15439/2020F94},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{9980612,
  author={Richards, Dale and Griffiths, Ian and Yeung, Kelvin and Cowell-Butler, Jennifer},
  booktitle={2022 IEEE 3rd International Conference on Human-Machine Systems (ICHMS)},
  title={Designing for Human-Machine Teams: A Methodological Enquiry},
  year={2022},
  volume={},
  number={},
  pages={1-4},
  abstract={Traditionally both Artificial Intelligence (Al) and Human Factors disciplines have tended to utilise their own methods and tools in order to achieve the design of Human-Machine Teams (HMT), with little interaction between the two approaches. However, in order to address both human and machine both disciplines can bring the design team together. In this paper we propose a shared co-design approach for HMT that facilitates a better understanding of system requirements and user interaction. By focusing on context rich scenarios and asking end users to walk through decisions they would make using the system it is possible for both AI and HF team members to obtain critical design information for the HMT. System intent and transparency are essential components within the HMT paradigm, and thus play a central role within the design team. This is particularly relevant when multiple AIs are involved in a decision that is then mediated by the machine. In such instances, the human component is a fundamental component and will often be required to approve or maintain awareness of the decision being generated. This is discussed using examples of user interface design and how the human can be embraced within such AI mediations.},
  keywords={},
  doi={10.1109/ICHMS56717.2022.9980612},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{10285208,
  author={Yu, Chuncheng and Song, Zhilai},
  booktitle={2023 2nd International Conference on Artificial Intelligence and Autonomous Robot Systems (AIARS)},
  title={Research and Development of Product Design Software Based on Improved Genetic Algorithm and CAE Simulation Software},
  year={2023},
  volume={},
  number={},
  pages={469-474},
  abstract={CAE software dynamically outputs the design method of drawings or models of a certain type of products or parts according to the parameters input from the outside. In this paper, the development of product design software based on improved GA (Genetic Algorithm) and CAE simulation software is studied. The product design software developed in this paper includes product family planning management tool, product scheme rapid design tool, multi-platform interface management tool, special collaborative design tool, database and knowledge base. Aiming at the problems of low resource reuse and difficult deployment in traditional simulation software resource allocation, an improved GA algorithm is proposed to solve the optimal mapping between simulation model and virtual machine. The experimental results show that the algorithm can effectively avoid premature algorithm, and with the improvement of resource utilization, it can provide better load balancing effect. The practicability and convenience of CAE process product design software and the feasibility and effectiveness of development method are verified. It has certain reference and guiding value for enterprises to develop this kind of software.},
  keywords={},
  doi={10.1109/AIARS59518.2023.00100},
  ISSN={},
