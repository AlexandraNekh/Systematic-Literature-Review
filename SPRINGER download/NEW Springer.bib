@article{DGM+23a,
  title = {Current Topics of Interdisciplinary Cooperation between Engineering and Human Sciences},
  author = {Dressler, Angela and Gerber, Nina and Menig, Angela and Pasnicu, Otilia and St{\"o}ver, Alina and Vogt, Joachim},
  year = {2023},
  month = mar,
  journal = {Zeitschrift f\"ur Arbeitswissenschaft},
  volume = {77},
  number = {1},
  pages = {7--22},
  issn = {0340-2444, 2366-4681},
  doi = {10.1007/s41449-023-00352-y},
  urldate = {2023-12-05},
  abstract = {Abstract             In this article, we highlight current research directions in the Technikpsychologie research area, using the example of the interdisciplinary research work of FAI (Work and Engineering Psychology Research Group at the Technical University of Darmstadt) and the articles included in this special issue. To this end, we relate the articles in this special issue from the research areas of road traffic planning (Hupfer et al.), usable IT security and privacy solutions (Renaud), social aspects of technically mediated communication (Diefenbach), human-centered interface design (Mucha et~al.), aviation safety (Santel), human-centered design of autonomous vehicles (Lindner \& Stoll), and perceptual psychology-oriented product design (Zandi \& Khanh) to current research projects at FAI.                            Practical Relevance                          Technical products only offer added value by efficiently supporting users in achieving their goals if they have been developed appropriately for the context of use and the individual characteristics of the users. The human-centered design of\textemdash especially technical\textemdash products reflects this through an iterative and participatory development process. In this article, we describe nine examples of such human-centered design of technology products. The research results and the methods presented provide insights for developers and decision-makers in the fields of transportation, IT, vehicle development and general product design.           ,              Zusammenfassung             In diesem Artikel zeigen wir aktuelle Forschungsrichtungen der Technikpsychologie am Beispiel der interdisziplin\"ar gepr\"agten Forschungsarbeit der FAI (Forschungsgruppe f\"ur Arbeits- und Ingenieurpsychologie an der Technischen Universit\"at Darmstadt) und den in diesem Themenheft enthaltenen Artikeln auf. Zu diesem Zweck setzen wir die Artikel dieses Themenheftes aus den Forschungsbereichen Verkehrsplanung (Hupfer et al.), nutzbare IT Sicherheit- und Privatsph\"arel\"osungen (Renaud), soziale Aspekte der technisch mediierten Kommunikation (Diefenbach), menschzentrierte Schnittstellengestaltung (Mucha et~al.), Flugsicherheit (Santel), menschzentrierte Gestaltung von autonomen Fahrzeugen (Lindner \& Stoll) sowie wahrnehmungspsychologisch orientierte Produktgestaltung (Zandi \& Khanh) zu aktuellen Forschungsprojekten der FAI in Verbindung.                            Praktische Relevanz                          Technische Produkte bieten nur dann einen Mehrwert, wenn sie Nutzende effizient in der Erreichung ihrer Ziele unterst\"utzen, wenn diese passend f\"ur den Nutzungskontext und die individuellen Charakteristiken der Nutzenden entwickelt wurden. Die menschzentrierte Gestaltung von \textendash ~insbesondere technischen~\textendash{} Produkten bildet dies durch den iterativ und partizipativ gepr\"agten Entwicklungsprozess ab. In diesem Artikel beschreiben wir insgesamt neun Beispiele f\"ur eine solche menschzentrierte Gestaltung von Technikprodukten. Die Forschungsergebnisse ebenso wie die vorgestellten Methoden liefern hierbei Erkenntnisse f\"ur Entwickler:innen und Entscheider:innen aus den Bereichen Verkehr, IT, Fahrzeugentwicklung sowie genereller Produktgestaltung.},
  langid = {english},
  file = {C:\Users\anekh\Zotero\storage\HRBH2S27\Dressler и др. - 2023 - Current topics of interdisciplinary cooperation be.pdf}
}

