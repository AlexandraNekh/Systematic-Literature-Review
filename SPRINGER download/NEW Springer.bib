@article{ZGHT22a,
  title = {An {{Interactive Evolutionary Design Method}} for {{Mobile Product Customization}} and {{Validation}} of {{Its Application}}},
  author = {Zeng, Dong and Guan, Ming and He, Maoen and Tian, Ziyu},
  year = {2022},
