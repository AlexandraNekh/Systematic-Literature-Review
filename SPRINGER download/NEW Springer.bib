@article{BAA23b,
  title = {Software Selection in Large-Scale Software Engineering: {{A}} Model and Criteria Based on Interactive Rapid Reviews},
  shorttitle = {Software Selection in Large-Scale Software Engineering},
  author = {Bjarnason, Elizabeth and {\AA}berg, Patrik and Ali, Nauman Bin},
  year = {2023},
