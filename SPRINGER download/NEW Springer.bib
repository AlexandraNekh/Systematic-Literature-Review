@article{SJP+21a,
  title = {Climate Change Adaptation in Conflict-Affected Countries: {{A}} Systematic Assessment of Evidence},
  shorttitle = {Climate Change Adaptation in Conflict-Affected Countries},
  author = {Sitati, A. and Joe, E. and Pentz, B. and Grayson, C. and Jaime, C. and Gilmore, E. and Galappaththi, E. and Hudson, A. and Alverio, G. Nagle and Mach, K. J. and Van Aalst, M. and Simpson, N. and Schwerdtle, P. Nayna and Templeman, S. and Zommers, Z. and Ajibade, I. and Chalkasra, L. S. Safaee and Umunay, P. and Togola, I. and Khouzam, A. and Scarpa, G. and {Global Adaptation Mapping Initiative Team} and De Perez, E. Coughlan},
