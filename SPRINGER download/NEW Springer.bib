  shorttitle = {Study Protocol},
  author = {Al Ghafri, Thamra and Anwar, Huda and Al Hinai, Eiman and Al Harthi, Thuraya and Al Jufaili, Fathiya and Al Siyabi, Reyadh and Al Harthi, Shamsa and Al Hasani, Said and Al Harthi, Mohammed and Al Harthi, Saud},
