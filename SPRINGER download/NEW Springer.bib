@article{AlA23a,
  title = {Emerging Technological Innovation in {{Gaza Strip}} Municipalities: An Entrepreneurial Approach},
  shorttitle = {Emerging Technological Innovation in {{Gaza Strip}} Municipalities},
  author = {AlAstal, Ashraf Y.},
