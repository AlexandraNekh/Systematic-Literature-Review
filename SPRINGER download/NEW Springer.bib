@article{SSMU21,
  title = {Validating Deep Learning Inference during Chest {{X-ray}} Classification for {{COVID-19}} Screening},
  author = {Sadre, Robbie and Sundaram, Baskaran and Majumdar, Sharmila and Ushizima, Daniela},
