@article{CS23e,
  title = {The Contested Role of {{AI}} Ethics Boards in Smart Societies: A Step towards Improvement Based on Board Composition by Sortition},
  shorttitle = {The Contested Role of {{AI}} Ethics Boards in Smart Societies},
  author = {Conti, Ludovico Giacomo and Seele, Peter},
  year = {2023},
  month = dec,
  journal = {Ethics and Information Technology},
  volume = {25},
  number = {4},
  pages = {51},
  issn = {1388-1957, 1572-8439},
  doi = {10.1007/s10676-023-09724-8},
  urldate = {2023-12-05},
  abstract = {Abstract                            The recent proliferation of AI scandals led private and public organisations to implement new ethics guidelines, introduce AI ethics boards, and list ethical principles. Nevertheless, some of these efforts remained a fa\c{c}ade not backed by any substantive action. Such behaviour made the public question the legitimacy of the AI industry and prompted scholars to accuse the sector of ethicswashing, machinewashing, and ethics trivialisation\textemdash criticisms that spilt over to institutional AI ethics boards. To counter this widespread issue, contributions in the literature have proposed fixes that do not consider its systemic character and are based on a top-down, expert-centric governance. To fill this gap, we propose to make use of               qualified informed lotteries               : a two-step model that transposes the documented benefits of the ancient practice of sortition into the selection of AI ethics boards' members and combines them with the advantages of a stakeholder-driven, participative, and deliberative bottom-up process typical of Citizens' Assemblies. The model permits increasing the public's legitimacy and participation in the decision-making process and its deliverables, curbing the industry's over-influence and lobbying, and diminishing the instrumentalisation of ethics boards. We suggest that this sortition-based approach may provide a sound base for both public and private organisations in smart societies for constructing a decentralised, bottom-up, participative digital democracy.},
  langid = {english},
  file = {C:\Users\anekh\Zotero\storage\27WCHEJM\Conti и Seele - 2023 - The contested role of AI ethics boards in smart so.pdf}
}

