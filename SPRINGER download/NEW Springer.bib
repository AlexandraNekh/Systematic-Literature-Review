  shorttitle = {Care {{Robot Orientation}}},
  author = {{Johansson-Pajala}, Rose-Marie and Thommes, Kirsten and Hoppe, Julia A. and Tuisku, Outi and Hennala, Lea and Pekkarinen, Satu and Melkas, Helin{\"a} and Gustafsson, Christine},
