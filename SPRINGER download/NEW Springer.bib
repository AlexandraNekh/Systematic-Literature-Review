@article{KJ22a,
  title = {Systematic Review on Ensuring the Global Food Security and Covid-19 Pandemic Resilient Food Systems: Towards Accomplishing Sustainable Development Goals Targets},
  shorttitle = {Systematic Review on Ensuring the Global Food Security and Covid-19 Pandemic Resilient Food Systems},
