@article{WKJ+22a,
  title = {Demand Sensing and Digital Tracking for Maternal Child Health ({{MCH}}) in {{Uganda}}: A Pilot Study for `{{E}}+{{TRA}} Health'},
  shorttitle = {Demand Sensing and Digital Tracking for Maternal Child Health ({{MCH}}) in {{Uganda}}},
  author = {Wang, Dawei and Kerh, Rhoann and Jun, Sungbum and Lee, Seokcheon and Mayega, Roy William and Ssentongo, Julius and Oumer, Andualem and Haque, Md and Brunese, Priyanka and Yih, Yuehwern},
  year = {2022},
