@article{GGB+23a,
  title = {Research {{Buddy}} Partnership in a {{MD}}\textendash{{PhD}} Program: Lessons Learned},
  shorttitle = {Research {{Buddy}} Partnership in a {{MD}}\textendash{{PhD}} Program},
  author = {Gould, Daniel J. and {Glanville-Hearst}, Marion and Bunzli, Samantha and Choong, Peter F. M. and Dowsey, Michelle M.},
  year = {2023},
