@article{AOST23a,
  title = {Design, Development, and Evaluation of a Virtual Reality Game-Based Application to Support Computational Thinking},
  author = {Agbo, Friday Joseph and Oyelere, Solomon Sunday and Suhonen, Jarkko and Tukiainen, Markku},
  year = {2023},
  month = apr,
