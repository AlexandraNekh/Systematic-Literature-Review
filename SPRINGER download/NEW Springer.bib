@article{CM22a,
  title = {The {{Futures}} of {{Europe}}: {{Society}} 5.0 and {{Industry}} 5.0 as {{Driving Forces}} of {{Future Universities}}},
  shorttitle = {The {{Futures}} of {{Europe}}},
  author = {Carayannis, Elias G. and {Morawska-Jancelewicz}, Joanna},
  year = {2022},
