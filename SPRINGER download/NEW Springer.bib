  author = {Bahemuka, Ubaldo M. and Okimat, Paul and Webb, Emily L. and Seeley, Janet and Ssetaala, Ali and Okech, Brenda and Oketch, Bertha and Kibengo, Freddie M. and Okello, Elialilia and Kwena, Zachary and Kuteesa, Monica O. and Price, Matt A. and Kaleebu, Pontiano and Grosskurth, Heiner and Fast, Pat and {LVCHR}},
